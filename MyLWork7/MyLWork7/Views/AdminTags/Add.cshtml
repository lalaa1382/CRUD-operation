@model MyLWork7.Models.ViewModels.AddTagRequest
<div>
    <div>
        <h4>Add new tag - Admim Functionallity</h4>
    </div>
</div>
<div>
    <form method="post">
        <div>
            <label class="form-label">Name</label>
            <input type="text" class="form-control" id="name" asp-for="Name" />
        </div>
        <div>
            <label class="form-label">Description</label>
            <input type="text" class="form-control" id="displayName" asp-for="DisplayName" />
        </div>
        <div>
            <button type="submit" class="btn btn-dark">Сохранить</button>
        </div>
    </form>
</div>
